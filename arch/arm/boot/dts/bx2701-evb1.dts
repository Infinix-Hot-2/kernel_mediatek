/dts-v1/;
#include "mt2701.dtsi"

/ {
	model = "MediaTek bx2701 Development Board";
	compatible = "mediatek,bx2701-evb1","mediatek,mt2701";

	chosen {
		bootargs = "earlyprintk";
	};

	memory {
		reg = <0x80000000 0x40000000>;
	};
};
